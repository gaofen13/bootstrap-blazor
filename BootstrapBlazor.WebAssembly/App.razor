<Router AppAssembly="@typeof(App).Assembly" AdditionalAssemblies="new [] {typeof(Shared._Imports).Assembly}" NotFoundPage="typeof(Pages.NotFound)">
    <Found Context="routeData">
        <RouteView RouteData="@routeData" DefaultLayout="@typeof(MainLayout)"/>
        <FocusOnNavigate RouteData="@routeData" Selector="h1" />
    </Found>
</Router>
